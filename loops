    <script>
        //for in  loop
        const a="jyoshna"
        for(let i in a){
            console.log(a[i])
        }
        //for of loop
        const bb=["jyosh","jyo","jyoshna"]
        for(let b of bb){
            console.log(b)
        }
        //while loop
        let i=0;
        while(i<5){
            console.log(i)
            i++
        }
        let j=2;
        let n=8;
        while(j<=n){
            //2<8 ....4<8 .....6<8.....8<=8
            console.log(i+1) //2+1=3  ....4+1=5...6+1=7.....8+1=9
            j=j+2;//2 is incremented by 2..then i is 4....then it is 6 .........then it is 8..10 here the condition violated
        }

        let c=0;
        do{
            console.log(c);
            c++;
        }
        while(c<5);

        for(let a=2;a<=8;a=a+2){
            if(a>4){
                break;
            }
            console.log(a+1)
        }


   
    </script>
